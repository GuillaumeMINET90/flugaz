{% extends 'base.html.twig' %}

{% block title %}TransferContainers{% endblock %}

{% block body %}
    <h1>TransferContainers</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ transfer_container.id }}</td>
            </tr>
            <tr>
                <th>Number</th>
                <td>{{ transfer_container.number }}</td>
            </tr>
            <tr>
                <th>Gaz</th>
                <td>{{ transfer_container.gaz }}</td>
            </tr>
            <tr>
                <th>Tare</th>
                <td>{{ transfer_container.tare }}</td>
            </tr>
            <tr>
                <th>Purchase_date</th>
                <td>{{ transfer_container.purchaseDate ? transfer_container.purchaseDate|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Return_date</th>
                <td>{{ transfer_container.returnDate ? transfer_container.returnDate|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Total_weight</th>
                <td>{{ transfer_container.totalWeight }}</td>
            </tr>
            <tr>
                <th>Volume</th>
                <td>{{ transfer_container.volume }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_transfer_containers_index') }}">back to list</a>

    <a href="{{ path('app_transfer_containers_edit', {'id': transfer_container.id}) }}">edit</a>

    {{ include('transfer_containers/_delete_form.html.twig') }}
{% endblock %}
